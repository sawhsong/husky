<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <package extends="default" name="bau" namespace="/bau" strict-method-invocation="false">
        <default-interceptor-ref name="ZebraDefaultStack"/>
        <!--Bau0202 - My Bank Accounts-->
        <action class="project.app.bau.bau0202.Bau0202Action" method="{1}" name="0202/*">
            <result name="list" type="debugDispatcherResult">/project/app/bau/bau0202/Bau0202List.jsp</result>
            <result name="edit" type="debugDispatcherResult">/project/app/bau/bau0202/Bau0202EditPop.jsp</result>
        </action>
        <!--Bau0204 - Announcement-->
        <action class="project.app.bau.bau0204.Bau0204Action" method="{1}" name="0204/*">
            <result name="list" type="debugDispatcherResult">/project/app/bau/bau0204/Bau0204List.jsp</result>
            <result name="detail" type="debugDispatcherResult">/project/app/bau/bau0204/Bau0204DetailPop.jsp</result>
        </action>
    </package>
</struts>
